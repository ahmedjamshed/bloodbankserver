    "ejs": "^3.0.1",
    "email-templates": "^6.1.1",
    "express": "^5.0.0-alpha.7",
    "express-graphql": "^0.9.0",
    "firebase-admin": "^9.1.1",
    "graphql": "^14.6.0",
    "graphql-compose": "^7.14.0",
    "graphql-compose-mongoose": "^7.3.4",
    "graphql-playground-middleware-express": "^1.7.18",
